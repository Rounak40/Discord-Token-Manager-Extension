{
    "manifest_version": 2,
    "name": "Discord Token Manager",
    "version": "1.2",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://discord.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  